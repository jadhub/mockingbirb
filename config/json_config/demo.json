[
  {
    "route": "/helloworld/{key}",
    "methods": ["get"],
    "responses": [
      {
        "uri": "/greet/me",
        "method": "get",
        "statusCode": 200,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
          "message": "Mockingbirb says hi"
        }
      },
      {
        "uri": "/test/json",
        "method": "get",
        "statusCode": 200,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
          "foo": {
            "key1": 1,
            "key2": true,
            "key3": [
              {
                "foo": "foo",
                "bar": true,
                "baz": [1, 2, "3"]
              }
            ]
          }
        }
      },{
        "uri": "/test/xml",
        "method": "get",
        "statusCode": 200,
        "headers": {
          "Content-Type": "application/xml; charset=utf-8"
        },
        "body": "<test><default><a>1</a><b>1</b></default></test>"
      }
    ]
  }
]